        "monolog/monolog": "^2.9",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "php-di/slim-bridge": "^3.3",
        "psr7-sessions/storageless": "^8.16",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.3",
        "symfony/cache": "^6.2",
